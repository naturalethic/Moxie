{
    "name": "moxie",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build && uglifyjs dist/index.js -b --in-situ && biome check --apply-unsafe --max-diagnostics=0 dist/index.js",
        "analyze": "vite-bundle-visualizer",
        "preview": "vite preview",
        "test": "vitest",
        "test:e2e": "cypress run",
        "test:e2e:gui": "cypress open",
        "test:mox:build": "GOBIN=$PWD/.mox/bin CGO_ENABLED=0 go install github.com/mjl-/mox@main",
        "test:mox:serve": ".mox/bin/mox localserve -dir .mox/data",
        "test:mox:reinit": "rm -rf .mox/data && .mox/bin/mox localserve -dir .mox/data --initonly",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@biomejs/biome": "^1.3.3",
        "autoprefixer": "^10.4.16",
        "cypress": "^13.4.0",
        "solid-js": "^1.8.5",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2",
        "uglify-js": "^3.17.4",
        "valibot": "github:fabian-hiller/valibot",
        "vite": "^4.5.0",
        "vite-bundle-visualizer": "^0.10.0",
        "vite-plugin-doctest": "^0.1.1",
        "vite-plugin-solid": "^2.7.2",
        "vite-tsconfig-paths": "^4.2.1",
        "vitest": "^0.34.6"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^7.6.0-alpha.3",
        "@storybook/addon-interactions": "^7.6.0-alpha.3",
        "@storybook/addon-links": "^7.6.0-alpha.3",
        "@storybook/blocks": "^7.6.0-alpha.3",
        "@storybook/testing-library": "^0.2.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "storybook": "^7.6.0-alpha.3",
        "storybook-solidjs": "^1.0.0-beta.2",
        "storybook-solidjs-vite": "^1.0.0-beta.2"
    }
}
